{
  "preset": "default",
  "indent": {
    "value": "  "
  },
  "lineBreak": {
    "after": {
      "VariableDeclaration": "2"
    }
  },
  "whiteSpace": {
    "before": {
      "ArrayExpressionClosing": 1
    },
    "after": {
      "ArrayExpressionOpening": 1
    }
  },
  "plugins": [
    "esformatter-braces",
    "esformatter-spaced-lined-comment"
  ]
}
